<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.havre.copymeter.ui"
    android:versionCode="12" android:versionName="2.0.0">
    <uses-sdk />
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.WAKE_LOCK"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:usesCleartextTraffic="true">
        <activity android:name="de.havre.copymeter.ui.main.PrinterListActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="de.havre.copymeter.ui.settings.PrinterListSettingsActifity"/>
        <activity android:name="de.havre.copymeter.ui.settings.PrinterSettingsActifity"/>
        <activity android:name="de.havre.copymeter.ui.settings.CounterSettingsActifity"/>
        <activity android:name="de.havre.copymeter.ui.settings.CounterListSettingsActifity"/>
        <activity android:name="de.havre.copymeter.ui.wizard.SemiAutomaticWizard"/>
        <activity android:name="de.havre.copymeter.ui.main.InfoActifity"/>
        <activity android:name="de.havre.copymeter.ui.settings.ConfigImportActifity"/>

    </application>


</manifest>